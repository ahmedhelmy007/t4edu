{% extends '::base.html.twig' %}

{% block body -%}
    <h1>EmployeesEvaluations list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idevaluation</th>
                <th>Createddate</th>
                <th>Modifieddate</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('employeesevaluations_show', { 'id': entity.idEvaluation }) }}">{{ entity.idEvaluation }}</a></td>
                <td>{% if entity.createdDate %}{{ entity.createdDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.modifiedDate %}{{ entity.modifiedDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('employeesevaluations_show', { 'id': entity.idEvaluation }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('employeesevaluations_edit', { 'id': entity.idEvaluation }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('employeesevaluations_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
